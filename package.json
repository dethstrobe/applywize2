{
  "name": "@redwoodjs/starter",
  "version": "1.0.0",
  "description": "A bare-bones RedwoodSDK starter",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && RWSDK_DEPLOY=1 wrangler deploy",
    "generate": "rw-scripts ensure-env && wrangler types",
    "check": "npm run generate && npm run types",
    "types": "tsc",
    "seed": "rwsdk worker-run ./src/scripts/seed.ts",
    "test": "playwright test --ui",
    "doc": "TEST2DOC=true playwright test --config=playwright-test2doc.config.ts"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@simplewebauthn/browser": "13.2.2",
    "@simplewebauthn/server": "13.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.548.0",
    "next-themes": "^0.4.6",
    "react": "19.3.0-canary-4fdf7cf2-20251003",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.3.0-canary-4fdf7cf2-20251003",
    "react-server-dom-webpack": "19.3.0-canary-4fdf7cf2-20251003",
    "rwsdk": "1.0.0-beta.22",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.13.10",
    "@cloudflare/workers-types": "4.20251004.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/vite": "^4.1.16",
    "@test2doc/playwright": "^1.0.1",
    "@types/node": "~24.5.2",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.8.3",
    "vite": "7.1.11",
    "wrangler": "4.42.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
