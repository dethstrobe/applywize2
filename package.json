{
  "name": "@redwoodjs/starter",
  "version": "1.0.0",
  "description": "A bare-bones RedwoodSDK starter",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && RWSDK_DEPLOY=1 wrangler deploy",
    "generate": "rw-scripts ensure-env && wrangler types",
    "check": "npm run generate && npm run types",
    "types": "tsc",
    "lint": "eslint .",
    "passkey:gen": "node ./src/scripts/generate-test-passkey.ts",
    "seed": "rwsdk worker-run ./src/scripts/seed.ts",
    "test": "playwright test --ui",
    "doc": "TEST2DOC=true playwright test --config=playwright-test2doc.config.ts",
    "icons": "lemon-lime-svgs"
  },
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@simplewebauthn/browser": "13.2.2",
    "@simplewebauthn/server": "13.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.548.0",
    "next-themes": "^0.4.6",
    "react": "19.2.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.2.1",
    "react-server-dom-webpack": "19.2.1",
    "rwsdk": "1.0.0-beta.22",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.13.10",
    "@cloudflare/workers-types": "4.20251004.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/vite": "^4.1.16",
    "@test2doc/playwright": "^1.2.0",
    "@test2doc/playwright-passkey": "^0.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "~24.5.2",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "better-sqlite3": "^12.4.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "lemon-lime-svgs": "^2.0.1",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.46.3",
    "vite": "7.1.11",
    "wrangler": "4.42.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "sharp",
      "workerd"
    ]
  },
  "lemonLimeSvgs": {
    "inputDir": "./other/svg-icons",
    "outputDir": "./public/images/icons",
    "typesDir": "./types",
    "spriteFilename": "sprite.svg",
    "typeFilename": "icons.d.ts",
    "verbose": false,
    "generateReadme": true
  }
}
